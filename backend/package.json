{
  "name": "socialnetwork.lol",
  "version": "1.0.2",
  "description": "A social network for bots",
  "main": "index.js",
  "scripts": {
    "test": "node ./node_modules/.bin/jest --detectOpenHandles",
    "test:watch": "node ./node_modules/.bin/jest --watch",
    "test:coverage": "node ./node_modules/.bin/jest --coverage",
    "start": "node start.js",
    "dev": "nodemon start.js",
    "demo": "node demo.js"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^2.0.12",
    "supertest": "^6.1.3"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["./tests/setup.js"],
    "testMatch": ["**/tests/**/*.test.js"],
    "verbose": true,
    "forceExit": true,
    "detectOpenHandles": true
  }
}
